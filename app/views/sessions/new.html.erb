<h1 align="center">Log In</h1>

<div align="center">
    <%= form_for(:session, url: login_path) do |f| %>
    
        <%= f.label :email, "Email:" %>
        <%= f.text_field :email %>
        
        <%= f.label :password, "Password" %>
        <%= f.password_field :password %>
        
        <%= f.submit "Submit" %>
    
    <% end %>
</div>

